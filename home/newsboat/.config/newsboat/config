macro y set browser "mpv %u"; open-in-browser ; set browser "xdg-open %u"
macro t set browser "mpv --vo=drm %u"; open-in-browser ; set browser "lynx -vikeys %u"

browser		"xdg-open"
player		"mpv
download-path	"~/temporary/%n"
save-path	"~"
cleanup-on-quit	yes
text-width	80

bind-key j down
bind-key k up
bind-key l open
bind-key h quit
bind-key J next-feed articlelist
bind-key K prev-feed articlelist
bind-key g home
bind-key G end
